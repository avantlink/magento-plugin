<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
    <policy id="script-src">
        <values>
            <value id="devdocs-base" type="host">https://developer.adobe.com</value>
            <value id="magento" type="host">https://magento.com</value>
            <value id="avmws-secure-base" type="host">https://cdn.avmws.com/</value>
            <value id="avmws-base" type="host">http://cdn.avmws.com/</value>
            <value id="avmws-dev" type="host">https://avmws-default.avantlink.net/</value>

        </values>
    </policy>
    <policy id="connect-src">
        <values>
            <value id="devdocs" type="host">https://developer.adobe.com</value>
            <value id="avmws-secure" type="host">https://cdn.avmws.com/</value>
            <value id="avmws" type="host">http://cdn.avmws.com/</value>
            <value id="avmws-dev2" type="host">https://avmws-default.avantlink.net/</value>
        </values>
    </policy>
    </policies>
</csp_whitelist>
